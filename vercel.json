{
  "name": "realwds-api",
  "version": 2,
  "routes": [
    {
      "src": "/",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api"
    },
    {
      "src": "/bing",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/bing/"
    },
    {
      "src": "/360/search",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/360/search"
    },
    {
      "src": "/360/hotSearch",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/360/hotSearch"
    },
    {
      "src": "/360/getAllCategories",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/360/getAllCategories"
    },
    {
      "src": "/360/getAppsByCategory",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/360/getAppsByCategory"
    },
    {
      "src": "/test/webhook",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/test/webhook"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/404"
    }
  ]
}
